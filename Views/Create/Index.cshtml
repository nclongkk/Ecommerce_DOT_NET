<form asp-controller="Create" asp-action="Post" method="post">
    <div class="form-group">
        <label for="exampleInputEmail1" style="color:white;">Title</label>
        <input type=" text" class="form-control" id="title" aria-describedby="emailHelp" placeholder="Enter title"
            name="Title">

    </div>
    <div class="form-group">
        <label for="image" style="color:white;">Image</label>
        <input type="text" class="form-control" id="image" aria-describedby="emailHelp" placeholder="Enter image url"
            name="Image">
    </div>
    <div class="form-group">
        <label for="url" style="color:white;">Url</label>
        <input type="text" class="form-control" id="url" aria-describedby="emailHelp" placeholder="Enter url"
            name="Url">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
